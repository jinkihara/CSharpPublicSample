<Window x:Class="MongoDBSample.StdoutSample"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="StdoutSample" Height="328.724" Width="544.681">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="25"/>
            <RowDefinition Height="25"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <DockPanel Grid.Row="0" HorizontalAlignment="Stretch">
            <Button Content="MongoDB起動" Click="Button_Click"/>
            <TextBox x:Name="commandInputTextBox" HorizontalAlignment="Stretch" KeyUp="commandInputTextBox_KeyUp"/>
        </DockPanel>
        <ComboBox x:Name="commandHistoryComboBox" Grid.Row="1" />
        <TextBox Grid.Row="2" x:Name="commandResultTextBox" HorizontalAlignment="Stretch" TextWrapping="Wrap" />
    </Grid>
</Window>
